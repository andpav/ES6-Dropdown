{
  "name": "es6-dropdown",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.config.dev.js --entry ./src/index.js --output-filename ./dist/main.js",
    "build": "webpack --mode production",
    "server": "nodemon ./server/index.js",
    "lint": "eslint ./src",
    "test": "jest"
  },
  "jest": {
    "notify": true,
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "tests/__mocks__/fileMock.js",
      "\\.(scss|css|less)$": "tests/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "src"
    ],
    "testRegex": "(.(test|spec)).(js|jsx|json|node)$",
    "roots": [
      "src"
    ]
  },
  "keywords": [],
  "author": "andpav",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^4.2.4",
    "jest-cli": "^22.4.3",
    "object-assign": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cors": "^2.8.4",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "jest": "^21.2.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.5",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.4"
  }
